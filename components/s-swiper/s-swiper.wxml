<!--components/s-swiper/s-swiper.wxml-->
<swiper class="swiper" circular autoplay interval="2000" indicator-dots indicator-color="#f1f1f1" indicator-active-color="#ff5777">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
